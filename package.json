{
  "name": "grappa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "mock-server": "mockserver -p 8081 -m mockserver",
    "start": "ng serve --port=4201",
    "start-all": "npm run mock-server & npm run start",
    "build": "ng build lib --configuration production",
    "watch": "ng build lib --watch",
    "publish": "npm run build && npm publish ./dist/lib",
    "test": "ng test --code-coverage=true --watch=false",
    "lint": "npx ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "core-js": "^3.26.0",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.8",
    "@angular-eslint/builder": "~18.0.0",
    "@angular-eslint/eslint-plugin": "~18.0.0",
    "@angular-eslint/eslint-plugin-template": "~18.0.0",
    "@angular-eslint/schematics": "~18.0.0",
    "@angular-eslint/template-parser": "~18.0.0",
    "@angular/cli": "^17.0.8",
    "@angular/compiler-cli": "^17.0.8",
    "@angular/language-service": "^17.0.8",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "ajv": "^8.17.1",
    "coverage": "^0.4.1",
    "eslint": "^8.53.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "50.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-cli": "~2.0.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-parallel": "^0.3.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "mockserver": "^3.1.1",
    "ng-packagr": "^17.0.3",
    "protractor": "~7.0.0",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "eslintIgnore": [
    "**/*.spec.ts",
    "**/*.mock.ts",
    "environment.prod.ts",
    "environment.ts",
    "test.ts"
  ]
